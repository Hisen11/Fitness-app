{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist",
      "strict": true
    },
    "include": ["src/**/*.ts", "src/server.js", "src/auth.js"],
    "exclude": ["node_modules"]
  }
  